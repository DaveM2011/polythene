{
  "name": "polythene-examples",
  "version": "0.2.35",
  "description": "",
  "main": "dist/js/examples.js",
  "module": "dist/js/examples.mjs",
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
